<body>
        
    <div class="loginCon row">
        <div class="col-6 loginBox">
            <img src="/images/log-in-girl.svg" alt="">
        </div>
        <div class="col-6 loginBox1">
            <div class="row">
                <div class="col-12 d-flex justify-content-center align-items-center">
                    <a href="/"><img class="logo" src="/images/quiz-land-logo.png" alt=""></a>
                </div>
                <div class="col-12 d-flex align-items-center flex-column">
                    <a href="/admin/login/googleAuth" target="_parent">
                        <div class="google-btn">
                            <div class="google-icon-wrapper">
                            <img class="google-icon" src="https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg"/>
                            </div>
                            <p class="btn-text"><b>Sign in with google</b></p>
                        </div>
                    </a>
                    <span style="font-size: 12px; margin: 15px 0; color:#ddd;">Log in and use the opportunities offered by Quiz Land</span>
                </div>
            </div>
        </div>
    </div>



